<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_magento_delivery_carrier_form" model="ir.ui.view">
        <field name="name">magento.delivery.carrier.form</field>
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="delivery.view_delivery_carrier_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Magento" name="magento">
                    <group name="magento_info">
                        <field name="magento_code"/>
                        <field name="magento_tracking_title"/>
                        <field name="magento_export_tracking"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
