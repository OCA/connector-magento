<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_magento_backend_form_options_active" model="ir.ui.view">
            <field name="name">magento.backend.form.options_active</field>
            <field name="model">magento.backend</field>
            <field name="inherit_id" ref="magentoerpconnect.view_magento_backend_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='default_category_id']" position="after">
                    <field name="product_active" />
                </xpath>
            </field>
        </record>

    </data>
</openerp>